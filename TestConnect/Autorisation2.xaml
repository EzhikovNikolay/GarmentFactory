<Window x:Class="TestConnect.Autorisation2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestConnect"
        mc:Ignorable="d"
        Title="Autorisation2" Height="600" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBox x:Name="Login" Width="200" Height="30"></TextBox>
            <PasswordBox x:Name="PasswordBox" Width="200" Height="30" Margin="0,10,0,0"></PasswordBox>
        </StackPanel>
        
        <TextBox x:Name="CodeBox" Width="200" Height="30" Margin="300,396,300,158"></TextBox>
        <Frame x:Name="framelog" NavigationUIVisibility="Hidden" Grid.RowSpan="2"/>
    </Grid>
</Window>
